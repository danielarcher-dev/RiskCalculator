        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Python Debugger: demo_accounts",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/risk_calculator/demo_accounts.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
                {
            "name": "Python Debugger: RiskCalculator",
            "type": "debugpy",
